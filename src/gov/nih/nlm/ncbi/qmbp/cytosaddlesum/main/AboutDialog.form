        </Property>
        <Property name="text" type="java.lang.String" value="&lt;html&gt;&lt;b&gt;&lt;i&gt;CytoSaddleSum&lt;/i&gt;: a functional enrichment analysis plugin for &lt;i&gt;Cytoscape&lt;/i&gt; based on sum-of-weights scores&lt;/b&gt;&lt;/html&gt;"/>
      </Properties>
    </Component>
